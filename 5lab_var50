#include <stdio.h>

int main() {
    int A[] = {3, 5, 2, 8, 1}; // исходный массив A
    int n = 5; // количество элементов в массиве
    int s = 0; // переменная для хранения суммы элементов
    
    // суммирование элементов массива A
    for (int i = 0; i < n; i++) {
        s = s + A[i];
    }
    
    // вывод суммы
    printf("Сумма элементов массива: %d\n", s);
    
    return 0;
}
