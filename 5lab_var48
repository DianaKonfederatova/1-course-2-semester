#include <stdio.h>

int main() {
    int n = 5; // предел, до какого числа будет перебор
    int s = 1; // переменная для хранения произведения
    
    // цикл для вычисления произведения чисел от 1 до n
    for (int i = 1; i <= n; i++) {
        s = s * i;
    }
    
    // вывод результата произведения
    printf("Произведение чисел от 1 до %d: %d\n", n, s);
    
    return 0;
}
