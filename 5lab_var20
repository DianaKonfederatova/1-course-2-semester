#include <stdio.h>

int main() {
    int A[100]; // массив для хранения простых чисел
    int m = 100; // максимальное количество простых чисел
    int i = 0, a, s, j, v = 50; 
    
    // поиск простых чисел меньше v
    for (i = 0, a = 2; a < v && i < m - 1; a++) {
        s = 0;
        for (j = 0; j < i; j++) {
            if (a % A[j] == 0) {
                s = 1;
                break;
            }
        }
        if (s == 0) {
            A[i++] = a;
        }
    }
     
    // вывод простых чисел
    for (j = 0; A[j] != 0; j++) {
        printf("%d ", A[j]);
    }
    
    return 0;
}
