#include <stdio.h>

int main() {
    int A[] = {3, 5, 2, 8, 1, 10, 6, 7, 4, 9}; // исходный массив A
    int s = 0; // переменная для хранения максимального значения
    
    // цикл для поиска максимального значения в массиве A
    for (int i = 0; i < 10; i++) {
        if (A[i] > s) {
            s = A[i];
        }
    }
    
    printf("Максимальное значение в массиве: %d\n", s);
    
    return 0;
}
